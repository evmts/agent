{
  "name": "@plue/snapshot",
  "version": "0.1.0",
  "description": "JJ (Jujutsu) snapshot system for Plue sessions",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "jj-native",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu"
      ]
    }
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "license": "Apache-2.0",
  "files": [
    "index.js",
    "index.d.ts",
    "*.node"
  ]
}
