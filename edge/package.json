{
  "name": "@plue/edge",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit",
    "dev": "wrangler dev --env dev",
    "deploy": "wrangler deploy --env production --var BUILD_VERSION:$(git rev-parse --short HEAD)",
    "deploy:dev": "wrangler deploy --env dev",
    "tail": "wrangler tail",
    "test": "vitest",
    "purge": "bun purge.ts all",
    "purge:tags": "bun purge.ts tags"
  },
  "dependencies": {},
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.11.1",
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.7.0",
    "vitest": "3.2.4",
    "wrangler": "^3.93.0"
  }
}
